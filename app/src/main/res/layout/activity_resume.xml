<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="tnpify.tnp_login.resume">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/test_text1"
        android:text="Curriculum Vite"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textSize="@dimen/abc_action_bar_progress_bar_size"/>

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="60dp"
        android:rowCount="5"
        android:columnCount="3"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/cv_analytics"
            android:layout_column="2"
            android:layout_row="0"
            android:layout_columnSpan="1"
            android:layout_gravity="center"
            android:textSize="@dimen/abc_text_size_title_material"
            android:textColorLink="@color/colorPrimaryDark" />
        <TextView
            android:id="@+id/cv_core"
            android:layout_column="2"
            android:layout_row="1"
            android:layout_columnSpan="1"
            android:layout_gravity="center"
            android:textSize="@dimen/abc_text_size_title_material"
            android:textColorLink="@color/colorPrimaryDark" />
        <TextView
            android:id="@+id/cv_consulting"
            android:layout_column="2"
            android:layout_row="2"
            android:layout_columnSpan="1"
            android:layout_gravity="center"
            android:textSize="@dimen/abc_text_size_title_material"
            android:textColorLink="@color/colorPrimaryDark" />
        <TextView
            android:id="@+id/cv_finance"
            android:layout_column="2"
            android:layout_row="3"
            android:layout_columnSpan="1"
            android:layout_gravity="center"
            android:textSize="@dimen/abc_text_size_title_material"
            android:textColorLink="@color/colorPrimaryDark" />
        <TextView
            android:id="@+id/cv_management"
            android:layout_column="2"
            android:layout_row="4"
            android:layout_columnSpan="1"
            android:layout_gravity="center"
            android:textSize="@dimen/abc_text_size_title_material"
            android:textColorLink="@color/colorPrimaryDark" />
    </GridLayout>
</RelativeLayout>
